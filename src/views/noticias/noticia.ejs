<%- include('../archivos/cabecera', { usuario: usuario }) %> 

<section>
    <div class="container">
        <div class="row">
            <div class="col-md-6">
                <h1> <%= noticia.tituloNoticia %> </h1>
                <br>
                <br>
                <p> <%= noticia.cuerpoNoticia %> </p>
                <br>
                <br>
                <h3>Fotos: </h3>
                <br>
                <% fotos.forEach(function(foto) { %>
                  <img src="/public/uploads/<%=noticia.tituloNoticia%>/<%=foto%>" width="200" height="200" class="img-fluid">
                <% }) %>
            </div>
          </div>
    </div>
  </section>

  <%- include('../archivos/footer') %> 
